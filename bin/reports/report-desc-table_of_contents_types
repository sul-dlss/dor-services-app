#!/usr/bin/env ruby
# frozen_string_literal: true

require_relative '../../config/environment'
require_relative '../../lib/unique_report'

UniqueReport.new(name: 'desc-table_of_content_types', dsids: ['descMetadata']).run do |ng_xml|
  ng_xml.xpath('//mods:tableOfContents/@type', mods: MODS_NS).map(&:content)
end
