#!/usr/bin/env ruby
# frozen_string_literal: true

require_relative '../../config/environment'
require_relative '../../lib/report'

Report.new(name: 'desc-physical_location_misc', dsids: ['descMetadata']).run do |ng_xml|
  !ng_xml.xpath('//mods:physicalLocation[@type="misc"]', mods: MODS_NS).empty?
end
